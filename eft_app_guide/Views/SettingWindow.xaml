<Window x:Class="eft_app_guide.Views.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eft_app_guide.Views"
        mc:Ignorable="d"
        
        xmlns:usercontrols="clr-namespace:eft_app_guide.Views.AsUserControls"
        xmlns:viewmodels="clr-namespace:eft_app_guide.ViewModels.ToUserControls"
        Title="Settings"
        Height="1080"
        Width="1920">

    <!--#region Dictionary Resources-->
    <Window.Resources>
        <ResourceDictionary Source="/Views/ResourceDictionaries/SettingResourceDictionary.xaml"/>
    </Window.Resources>
    <!--#endregion-->

    <Grid>
        <ContentControl Content="{Binding CurrentUserControl, Mode=OneWay}" />
    </Grid>
</Window>
